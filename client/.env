# Agenoverse Client Environment Configuration
# Copy this file to .env.local for local development

# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=10000

# App Configuration
VITE_APP_NAME=Agenoverse
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Agenoverse - Digital Solutions Company
VITE_APP_ENV=development

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false
VITE_ENABLE_DEBUG_MODE=true

# External Services (Optional - add your own)
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=
VITE_HOTJAR_ID=

# Company Information
VITE_COMPANY_EMAIL=info@agenoverse.com
VITE_COMPANY_PHONE=+1234567890
VITE_COMPANY_ADDRESS=Your Company Address
VITE_LINKEDIN_URL=https://linkedin.com/company/agenoverse
VITE_TWITTER_URL=https://twitter.com/agenoverse
VITE_GITHUB_URL=https://github.com/agenoverse

# Performance
VITE_CACHE_TTL=300000
